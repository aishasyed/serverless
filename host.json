{
  "logger": {
    "categoryFilter": {
      "defaultLevel": "Information",
      "categoryLevels": {
        "Host.Results": "Error",
        "Function": "Error",
        "Host.Aggregator": "Information"
      }
    }
  }, 
  "functionTimeout": "00:05:00",
  "tracing": {
    "consoleLevel": "verbose",
    "fileLoggingMode": "debugOnly"
  },
  "applicationInsights": {
    "applicationInsights": {
        "sampling": {
          "isEnabled": false,
          "maxTelemetryItemsPerSecond" : 5
        }
    }
  },
  "aggregator": {
    "aggregator": {
        "batchSize": 1000,
        "flushTimeout": "00:00:30"
    }
  }
}